{
  "name": "react-search",
  "version": "1.0.3",
  "description": "a simple search component",
  "main": "lib/search.js",
  "jsnext:main": "src/search.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/StevenIseki/react-search.git"
  },
  "author": "Steven Iseki <stevenisekimartin@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "react": "^0.14.0"
  },
  "devDependencies": {
    "ava": "^0.3.0",
    "babel-core": "^5.6.7",
    "babel-eslint": "^4.1.4",
    "babel-loader": "^5.1.4",
    "express": "^4.7.2",
    "jsdom": "^7.0.2",
    "pre-commit": "^1.1.2",
    "react": "^0.14.0",
    "react-addons-test-utils": "^0.14.2",
    "react-dom": "^0.14.0",
    "standard": "^5.3.1",
    "webpack": "^1.9.12"
  },
  "scripts": {
    "test": "ava test/*.js",
    "build": "babel src --out-dir lib",
    "example": "webpack --config ./example/webpack.config.js",
    "serve": "node ./example/server.js",
    "start": "npm run example && npm run serve",
    "style": "standard"
  },
  "pre-commit": [
    "style",
    "test"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib",
      "example/public"
    ]
  },
  "keywords": [
    "javascript",
    "react",
    "reactjs",
    "react-component",
    "autocomplete",
    "search autocomplete",
    "auto complete",
    "search",
    "react-search",
    "react search",
    "search control",
    "searchbox",
    "search-box"
  ]
}
